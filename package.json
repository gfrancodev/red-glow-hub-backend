{
  "name": "player",
  "version": "1.0.0",
  "description": "Player platform API with Node.js, TypeScript, Prisma, and MongoDB",
  "main": "dist/index.js",
  "module": "index.ts",
  "type": "module",
  "private": true,
  "scripts": {
    "dev": "dotenvx run -- tsx watch src/index.ts",
    "build": "tsc",
    "start": "dotenvx run -- dotenvx run -- node dist/index.js",
    "test": "dotenvx run -- vitest",
    "test:e2e": "dotenvx run -- vitest --run src/modules/*/e2e",
    "test:e2e:auth": "dotenvx run -- vitest --run src/modules/auth/e2e",
    "test:e2e:watch": "dotenvx run -- vitest --watch src/modules/*/e2e",
    "prisma:generate": "dotenvx run -- prisma generate",
    "prisma:push": "dotenvx run -- prisma db push",
    "prisma:studio": "dotenvx run -- prisma studio",
    "seed": "dotenvx run -- tsx src/shared/db/seed.ts",
    "lint": "eslint . --ext .ts,.js --fix",
    "lint:check": "eslint . --ext .ts,.js",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "format:lint": "npm run format && npm run lint",
    "type-check": "tsc --noEmit"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.911.0",
    "@aws-sdk/s3-request-presigner": "^3.911.0",
    "@cloudflare/workers-types": "^4.20251011.0",
    "@dotenvx/dotenvx": "^1.51.0",
    "@prisma/client": "^6.17.1",
    "@types/swagger-ui-express": "^4.1.8",
    "argon2": "^0.44.0",
    "axios": "^1.12.2",
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "express": "^5.1.0",
    "express-rate-limit": "^8.1.0",
    "helmet": "^8.1.0",
    "http-status-codes": "^2.3.0",
    "ioredis": "^5.8.1",
    "jsonwebtoken": "^9.0.2",
    "mercadopago": "^2.9.0",
    "pino": "^10.0.0",
    "pino-http": "^11.0.0",
    "pino-pretty": "^13.1.2",
    "prisma": "^6.17.1",
    "rate-limit-redis": "^4.2.2",
    "swagger-ui-express": "^5.0.1",
    "uuid": "^13.0.0",
    "zod": "^4.1.12",
    "zod-openapi": "^5.4.3"
  },
  "devDependencies": {
    "@types/compression": "^1.8.1",
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.3",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^24.8.0",
    "@types/supertest": "^6.0.3",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.46.1",
    "@typescript-eslint/parser": "^8.46.1",
    "@vitest/ui": "3.2.4",
    "eslint": "^9.37.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-security": "^3.0.1",
    "eslint-plugin-unicorn": "^61.0.2",
    "nodemon": "^3.1.10",
    "prettier": "^3.6.2",
    "supertest": "^7.1.4",
    "tsx": "^4.20.6",
    "vitest": "^3.2.4"
  }
}
